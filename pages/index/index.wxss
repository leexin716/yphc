/**index.wxss**/
.price_content{
  margin-top: 10px;
  display: flex;
  flex-direction: column
}
.price_tabs{
  display: flex;
  justify-content: space-around;
  background: #fff;
  font-size: 32rpx;
  color: #666;
  padding-top: 10px
}
.price_tabs view{
  padding-bottom: 10px;
}
.active{
  color: #FF5500;
  border-bottom:2px solid #FF5500
}
.price_logo{
  background: #fff;
  margin-top: 1px;
  display: flex;
  font-size: 28rpx;
  color: #666;
  justify-content: space-around
}
.price_logo view{
  text-align: center;
  padding: 10rpx 0 5rpx 0
}
.price_car{
  margin-top: 1px;
  background: #fff;
  display: flex;
}
.price_car-left{
  width: 50%;
}
.price_car-left-in{
  padding:30rpx;
}
.price_car-left-in view:first-child{
  font-size: 32rpx;
  font-weight: bold
}
.price_car-left-in view:nth-child(2){
  font-size: 28rpx;
  color: #999;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.price_car-left-in view:nth-child(3){
  font-size: 28rpx;
  color: #999
}
.price_car_text{
  font-size: 24rpx;
  color: #fff;
  background: #FF5500;
  padding: 0 5rpx
}
.price_car-right{
  width: 50%;
  display: flex;
  flex-direction: column;
  justify-content: space-around
}
.pricecar-item{
  display: flex;
  padding-right: 30rpx;
  justify-content: space-between
}
.pricecar-item-word{
  width: 50%
}
.pricecar-name{
  font-size: 24rpx;
  font-weight: bold
}
.pricecar-mou{
  font-size: 22rpx;
  color: #999
}
.free_title{
  font-size: 28rpx;
  color: #666;
  padding: 30rpx 0;
  text-align: center
}
.free_content{
  display: flex;
  justify-content: space-between;
  flex-flow: wrap;
}
.free_item{
  width: 50%;
  background: #fff;
  margin-top: 1px;
}
.free_item:nth-child(2n+1){
  margin-left: -1px;
  margin-right: 1px
}
.free_item:nth-child(2n+2){
  margin-left: 0px;
  margin-right: 0px
}
.free_itemer{
  padding: 30rpx;
  display: flex;
  flex-direction: column;
}
.free_itemer_cont{
  font-size: 28rpx;
  color: #999;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.free_itemer_title{
  font-size: 32rpx;
  color: #333;
}
.story_content{
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  background: #fff;
  margin-bottom: 1px
}
.story_right{
  width: 58%
}
.story_right view{
  font-size: 28rpx;
  color: #333
}
.story_right_content{
  font-size: 24rpx;
  color: #999!important;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  /* height: 120rpx; */
}
.searcher{
  display: flex;
  align-items: center;
  justify-content: space-around;
  position: fixed;
  top: 0;
  z-index: 99;
  width: 100%;
  height: 80rpx;
  color: #fff;
  font-size: 28rpx
}
.search_left{
  font-size:28rpx;
  color: #fff;
  background:rgba(255,255,255,0.6);
  padding:7rpx 0;
  border-radius:30rpx;
  text-align: center
}
.backgd{
  background: #FF5500;
  transform: background .3s linear
}
.cityclass{
  width: 12%;
}
.citycontennt{
  display: flex;
  justify-content: space-around
}