/* pages/funpartylist/funpartylist.wxss */
.timer{
  font-size: 26rpx;
  color: #666;
  padding: 20rpx 0;
  text-align: center
}
.content{
  width: 85%;
  margin: auto;
  background: #fff;
  border-radius: 10px;
  padding: 25rpx
}
.titile{
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.middle{
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  border-bottom: 1px solid #eee;
  font-size: 28rpx;
  color: #666;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  -webkit-line-clamp: 2;
}
.lookmore{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #666;
  padding: 15rpx 15rpx 0 15rpx
}